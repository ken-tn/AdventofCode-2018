local in = [[abcdef
asdasdasasd
asdfasdfzxcvzxv
adsfasdfasdfsfasdf
fffffffff
szxcvxvvzxcvvvvvvv
fsdfafffff
]]

function getLines(str)
	local linesTable = {}
	
	for number in str:gmatch("[^\r\n]+") do
   		table.insert(linesTable, number)
	end
	
	return linesTable
end

function processLinesForThree(linesTable, instancesOfLetters)
	local threeCount = 0
	
	for _, line in pairs(linesTable) do
		local letterTable = {}
		for letter in pairs(line) do
			for _, v in pairs(letterTable) do
				local instances = 0
				if letter == v then
					instances = instances + 1
					if instances == 3 then
						threeCount = threeCount + 1
					end
				end
			end
			table.insert(letterTable, letter)
		end
	end
	
	return threeCount
end

function main()
	lines = getLines()
	twoCount = processLines(lines, 2)
	threeCount = processLines(lines, 3)
	print(twoCount * threeCount)
end

main()
